{
  "name": "@task-queue/client",
  "version": "1.0.0",
  "description": "TypeScript client SDK for the Task Queue API",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "generate": "openapi-ts -i ../../docs/openapi.yaml -o src/generated",
    "build": "tsc",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "task-queue",
    "api-client",
    "typescript",
    "openapi"
  ],
  "license": "MIT",
  "devDependencies": {
    "@hey-api/openapi-ts": "^0.73.0",
    "typescript": "^5.7.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
